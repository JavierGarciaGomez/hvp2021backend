{
  "name": "hvp2021backend",
  "version": "1.0.0",
  "description": "HVP2021 Backend - Hospital Veterinario Peninsular management system",
  "main": "app.js",
  "dependencies": {
    "@types/faker": "^6.6.9",
    "bcrypt": "^5.0.1",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^2.5.0",
    "cookie-session": "^2.0.0",
    "cors": "^2.8.5",
    "dayjs": "^1.11.13",
    "dotenv": "^10.0.0",
    "env-var": "^7.5.0",
    "express": "^4.17.2",
    "express-validator": "^6.14.0",
    "faker": "^6.6.6",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.1.5",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.13",
    "passport": "^0.5.2",
    "passport-google-oauth20": "^2.0.0",
    "typescript": "^5.3.3",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-session": "^2.0.49",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/mongoose": "^5.11.97",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.10.5",
    "@types/nodemailer": "^6.4.14",
    "@types/passport": "^1.0.16",
    "@types/passport-google-oauth20": "^2.0.16",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0"
  },
  "scripts": {
    "start": "node dist/app.js",
    "build": "node --max-old-space-size=2048 ./node_modules/.bin/tsc",
    "heroku-postbuild": "yarn build",
    "postinstall": "yarn build",
    "dev": "tsnd --respawn --clear src/index.ts",
    "dev:debug": "tsnd --inspect --respawn --clear src/index.ts",
    "docker:test": "docker-compose --profile test --env-file .env.test up -d",
    "docker:dev": "docker-compose --profile dev up -d",
    "test": "npm run docker:test && jest",
    "test:ci": "jest",
    "test:watch": "npm run docker:test && jest --watch",
    "test:coverage": "npm run docker:test && jest --coverage",
    "seed:attendanceRecords": "tsc && node build/scripts/attendanceRecordSeed.js",
    "seed:customerRFCs": "tsc && node build/scripts/customerRFCSeed.js",
    "seed:customSeed": "tsc && node build/scripts/customSeed.js",
    "seed:notifications": "tsnd src/scripts/notificationSeed.ts",
    "parse:tickets": "tsnd --project tsconfig.json src/shared/scripts/commissions/parseTickets.ts",
    "parse:payrolls": "tsnd --project tsconfig.json src/shared/scripts/payrolls/parsePayrolls.ts",
    "generate:activityRegisters": "tsnd --project tsconfig.json src/shared/scripts/others/generateActivityRegisters.ts",
    "db:copy-prod": "bash scripts/copy-prod-to-dev.sh"
  },
  "repository": "git+https://github.com/JavierGarciaGomez/hvp2021backend.git",
  "keywords": [],
  "author": "Javier Garcia <javieron.garcia@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JavierGarciaGomez/hvp2021backend/issues"
  },
  "homepage": "https://github.com/JavierGarciaGomez/hvp2021backend#readme",
  "engines": {
    "node": ">=20.0.0"
  }
}
