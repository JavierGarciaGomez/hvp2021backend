  "scripts": {
    "start": "node build/index.js",
    "build": "rimraf ./build && tsc",
    "dev": "tsnd --respawn --clear src/index.ts",
    "seed:attendanceRecords": "tsc && node build/scripts/attendanceRecordSeed.js",
    "seed:customerRFCs": "tsc && node build/scripts/customerRFCSeed.js",
    "seed:customSeed": "tsc && node build/scripts/customSeed.js",
    "preinstall": "yarn add typescript tsc",
    "docker:test": "docker compose -f docker-compose.test.yml --env-file .env.test up -d",
    "test": "npm run docker:test && jest",
    "test:watch": "npm run docker:test && jest --watch",
    "test:coverage": "npm run docker:test && jest --coverage",
    "seed:notifications": "tsnd src/scripts/notificationSeed.ts",
    "postinstall": "yarn add typescript tsc",
    "deploy": "git add . && git commit -m Heroku && git push heroku master",
    "tsc": "tsc"
  },
